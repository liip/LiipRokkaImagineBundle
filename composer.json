{
    "name": "liip/rokka-imagine-bundle",
    "description": "This bundle provides an integration between ImagineBundle and Rokka service.",
    "keywords": [ "imagine", "image", "rokka", "rokka.io", "pictures", "photos", "bundle", "symfony", "liip" ],
    "homepage": "https://liip.ch",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.1",
        "symfony/thanks": "^1.0",
        "symfony/console": "^3.4|^4.0",
        "monolog/monolog": "^1.23",
        "rokka/client": "^1.3",
        "liip/imagine-bundle": "^2.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2.3",
        "symfony/phpunit-bridge": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Liip AG",
            "homepage": "http://www.liip.ch/"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/liip/LiipRokkaImagineBundle/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Liip\\RokkaImagineBundle\\": ""
        },
        "exclude-from-classmap": [ "/Tests/" ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
